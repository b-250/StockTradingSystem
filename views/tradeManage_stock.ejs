            <!-- Main content -->
            <section class="content">
                <form action="#" id="tradeManage_stock_form" method="post">
                    <div class="box">
                        <div class="box-header">
                            <h3 class="box-title">查找股票</h3>
							<a ><%=errMsg%></a>
                        </div>
                        <div class="box-body">
                            <div class="input-group margin">
                            <input type="text" class="form-control" name="stock_id" placeholder="股票ID？.." /> 
                            <span class="input-group-btn">
                                <button type="submit" class="btn btn-info btn-flat">查找</button>
                            </span></div>
                            <!-- /input-group -->
                        
						<table id="example2" class="table table-bordered table-hover">
							<thead>
								<tr>
									<th> 股票名称 </th>
									<th> 交易日期 </th>
									<th> 交易时间 </th>
									<th> 成交价格(元) </th>
									<th> 涨跌幅 </th>
									<th> 成交手 </th>
									<th> 成交金额(元) </th>
									<th> 买卖类型 </th>
									<th> 买家 </th>
									<th> 卖家 </th>
								</tr>
							</thead>
							<tbody>
								<% for(var i=0;i<data.length;i++){ %>
									<tr>
										<td> <%= data[i].code %></td>
										<td> <%= data[i].date %> </td>
										<td> <%= data[i].time %> </td>
										<td> <%= data[i].price %> </td>
										<td> <%= data[i].change %> </td>
										<td> <%= data[i].volume %> </td>
										<td> <%= data[i].amount %> </td>
										<td> <%= data[i].type %> </td>
										<td> <%= data[i].purchaser %> </td>
										<td> <%= data[i].seller %> </td>
									</tr> 
								<%}%>
							</tbody>
                        </table>                  
                        <!-- /.box-body -->
						</div>
					</div>
                    <!-- Your Page Content Here -->
